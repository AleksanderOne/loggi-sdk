{
  "name": "loggi-sdk",
  "version": "1.0.0",
  "description": "Server-side SDK dla Loggi-App - centralny system logowania",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AleksanderOne/loggi-sdk.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "import": "./dist/next/index.mjs",
      "require": "./dist/next/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "logging",
    "nextjs",
    "sdk",
    "monitoring"
  ],
  "author": "AleksanderOne",
  "license": "MIT",
  "peerDependencies": {
    "next": ">=14.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}